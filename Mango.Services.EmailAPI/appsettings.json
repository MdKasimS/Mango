{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localDb)\\MSSQLLocalDB;Database=Mango_Email;Trusted_Connection=true;TrustServerCertificate=true"
  },
  "MessageQueue": {
    "Provider": "RabbitMQ",
    "RabbitMQ": {
      "ConnectionString": "amqp://guest:guest@localhost:5672"
    },
    "AzureServiceBus": {
      "ConnectionString": "Endpoint=sb://your-servicebus.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=XXXX"
    },
    "Kafka": {
      "BootstrapServers": "localhost:9092"
    }
  },
  "TopicAndQueueNames": {
    "EmailShoppingCartQueue": "emailshoppingcart"
  }
}
